<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.UserGiftMapper">

   <insert id="insert" parameterType="Achievement" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into t_user_gift
		(
               `user_id`,
               `gift_num`,
               `create_time`
		)
		values
		(
               #{userId},
               #{giftNum},
               #{createTime}
		)
	</insert>

	<delete id="delete">
		delete from t_user_gift where  id= #{id}
	</delete>
	

	<select id="findAll" resultType="Achievement">
	    select * from t_user_gift
	</select>


</mapper>