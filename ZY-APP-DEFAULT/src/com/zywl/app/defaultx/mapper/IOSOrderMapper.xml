<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.IOSOrderMapper">

    <insert id="insert" parameterType="IOSOrder" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        insert into r_ios_order
        (`user_id`,
         `product_id`,
         `order_no`,
         `in_app`,
         `price`,
         `create_time`)
        values (#{userId},
                #{productId},
                #{orderNo},
                #{inApp},
                #{price},
                #{createTime})
    </insert>


    <select id="findAll" resultType="IOSOrder">
        select *
        from r_ios_order
    </select>


</mapper>