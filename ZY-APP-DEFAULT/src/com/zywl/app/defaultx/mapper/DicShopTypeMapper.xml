<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.DicShopTypeMapper">



    <select id="findAll" resultType="DicShopType">
        select *
        from dic_shop_type
    </select>

    <select id="findByShopType" resultType="DicShopType">
        select *
        from dic_shop_type where shop_type = #{shopType}
    </select>

</mapper>