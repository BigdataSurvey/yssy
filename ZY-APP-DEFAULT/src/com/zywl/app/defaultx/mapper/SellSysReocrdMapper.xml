<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.SellSysRecordMapper">

   <insert id="insert" parameterType="SellSysRecord" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into r_sell_sys_record
		(
               `user_id`,
                `item_id`,
                `number`,
               `order_no`,
               `total_amount`,
               `create_time`
               
		)
		values
		(
               #{userId},
               #{itemId},
               #{number},
               #{orderNo},
               #{totalAmount},
               #{createTime}
        
		)
	</insert>
	
	
	<delete id="delete">
		delete from r_sell_sys_record where  id= #{id}               
	</delete>
	
	<select id="findOne" resultType="SellSysRecord">
	    select * from r_sell_sys_record where id = #{id}
	</select>
	
	
	
	<select id="findAll" resultType="ExchangeRecord">
	    select * from r_sell_sys_record
	</select>
	
	<select id="findByUserId" resultType="ExchangeRecord">
	    select * from r_sell_sys_record where user_id = #{userId}
	</select>
	
</mapper>