<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.CanLoginMapper">

   <insert id="insert" parameterType="CanLogin" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into t_can_login
		(
               `id_card`
		)
		values
		(
               #{id_card}
        	#{type}
		)
	</insert>
	

	
	
	<select id="findAll" resultType="CanLogin">
	    select * from t_can_login
	</select>


</mapper>