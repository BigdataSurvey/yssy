<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.DicPrizeMapper">


	<select id="findAll" resultType="DicPrize">
	    select * from dic_prize
	</select>


	<select id="findByUserId" resultType="DicPrize">
		select * from dic_prize
	</select>

	<update id="updatePosition" parameterType="RedPosition">
		UPDATE dic_prize
		SET num_total = total
		WHERE id = #{id}
	</update>
	

</mapper>