<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.UserOpenMineRecordMapper">

   <insert id="insert" parameterType="UserOpenMineRecord" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into r_user_open_mine_record
		(
               `user_id`,
               `order_no`,
               `amount`,
               `create_time`

               
		)
		values
		(
               #{userId},
               #{orderNo},
               #{amount},
               #{createTime}
		)
	</insert>
	
	
	
	<select id="findAll" resultType="UserOpenMineRecord">
	    select * from r_user_open_mine_record
	</select>
	
	
   		
   	
	
	
	

</mapper>