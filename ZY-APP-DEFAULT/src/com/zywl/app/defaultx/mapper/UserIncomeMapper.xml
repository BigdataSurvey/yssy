<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.UserIncome">

    <insert id="insert" parameterType="UserIncome" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        insert into t_user_income
        (`user_id`,
         `receive_income`,
         `unreceive_income`,
         `begin_time`,
         `end_time`)
        values (#{userId},
                #{receiveIncome},
                #{unreceiveIncome},
                #{beginTime},
                #{endTime})
    </insert>

    <update id="update" parameterType="UserIncome">
        update t_user_income
        <set>
            <if test="receiveIncome!=null">
                receive_income = #{receiveIncome},
            </if>
            <if test="unreceiveIncome!=null">
                unreceive_income = #{unreceiveIncome},
            </if>
            <if test="beginTime!=null">
                begin_time = #{beginTime},
            </if>
            <if test="endTime!=null">
                end_time = #{endTime}
            </if>
        </set>
        where user_id= #{userId}
    </update>


    <select id="findAll" resultType="Achievement">
        select *
        from t_user_income
    </select>


</mapper>