<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.VipReceiveRecordMapper">

    <insert id="insert" parameterType="VipReceiveRecord" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        insert into r_vip_receive_record
        (    `user_id`,
             `vip_level`,
             `reward`,
             `order_no`,
             `create_time`,
             `update_time`
        )
        values
            (
                #{userId},
                #{vipLevel},
                #{reward},
                #{orderNo},
                #{createTime},
                #{updateTime}
            )
    </insert>


    <delete id="delete">
        delete from r_vip_receive_record where id= #{id}
    </delete>

    <select id="findByConditions" resultType="VipReceiveRecord">
        select * from r_vip_receive_record where user_id = #{userId} and vip_level = #{vipLevel}
    </select>

    <select id="findAll" resultType="VipReceiveRecord">
        select * from r_vip_receive_record
    </select>


</mapper>