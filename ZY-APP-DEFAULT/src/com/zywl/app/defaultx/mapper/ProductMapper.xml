<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.ProductMapper">

   <insert id="insert" parameterType="Product" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into t_product
		(
               `product_name`,
               `icon`,
               `description`,
		        `days`,
               `price`,
               `number`,
               `status`

               
		)
		values
		(
               #{productName},
               #{icon},
               #{description},
		        #{days},
               #{price},
               #{number},
               #{status}
		)
	</insert>
	
	
	
	<select id="findAll" resultType="Product">
	    select * from t_product where status=1
	</select>



	
	
	

</mapper>