<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.CompleteAchievementRecordMapper">

   <insert id="insert" parameterType="CompleteAchievementRecord" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into r_complete_achievement_record
		(
               `user_id`,
               `achievement_id`,
               `order_no`,
               `reward`,
             	`create_time`
		)
		values
		(
               #{userId},
               #{achievementId},
               #{orderNo},
               #{reward},
               #{createTime}
             
             
		)
	</insert>
	
	
	
	<delete id="delete">
		delete from r_complete_achievement_record where  id= #{id}               
	</delete>
	
	<select id="findOne" resultType="CompleteAchievementRecord">
	    select * from r_complete_achievement_record where id = #{id}
	</select>
	
	
	<select id="findByUserId" resultType="CompleteAchievementRecord">
	    select * from r_complete_achievement_record where user_id = #{userId}
	</select>
	
	<select id="findAll" resultType="CompleteAchievementRecord">
	    select * from r_complete_achievement_record
	</select>
	
	
</mapper>