<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.DicShopMapper">



    <select id="findAll" resultType="DicShop">
        select *
        from dic_shop
    </select>

    <select id="findByShopType" resultType="DicShop">
        select *
        from dic_shop where shop_type = #{shopType}
    </select>


</mapper>