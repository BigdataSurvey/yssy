<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zywl.app.defaultx.mapper.UserGiftRecordMapper">

   <insert id="insert" parameterType="Achievement" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
			insert into r_user_gift_record
		(
               `user_id`,
               `type`,
               `price`,
               `order_no`,
               `number`,
			   `create_time`
		)
		values
		(
               #{userId},
               #{type},
               #{price},
               #{orderNo},
        	   #{number},
			   #{createTime}
		)
	</insert>

	
	<delete id="delete">
		delete from r_user_gift_record where  id= #{id}
	</delete>
	
	<select id="findByType" resultType="Achievement">
	    select * from r_user_gift_record where type = #{type}
	</select>
	
	
	<select id="findAll" resultType="Achievement">
	    select * from r_user_gift_record
	</select>


</mapper>